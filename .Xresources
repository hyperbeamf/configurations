! urxvt configs - so far they are extremely small
URxvt.scrollBar: false
URxvt.font: xft:Office Code Pro Medium:size=11
URxvt.letterSpace: -1
URxvt.internalBorder: 15

! X colors.
! Generated by 'wal'
*foreground:        #FCF3C0
*background:        #312F2E
*.foreground:       #FCF3C0
*.background:       #312F2E
emacs*foreground:   #FCF3C0
emacs*background:   #312F2E
URxvt*foreground:   #FCF3C0
XTerm*foreground:   #FCF3C0
UXTerm*foreground:  #FCF3C0
URxvt*background:   [100]#312F2E
XTerm*background:   #312F2E
UXTerm*background:  #312F2E
URxvt*cursorColor:  #FCF3C0
XTerm*cursorColor:  #FCF3C0
UXTerm*cursorColor: #FCF3C0
URxvt*borderColor:  [100]#312F2E

! Colors 0-15.
*.color0: #312F2E
*color0:  #312F2E
*.color1: #B7BA25
*color1:  #B7BA25
*.color2: #72C835
*color2:  #72C835
*.color3: #D2859A
*color3:  #D2859A
*.color4: #FA4832
*color4:  #FA4832
*.color5: #82A497
*color5:  #82A497
*.color6: #F9BC2E
*color6:  #F9BC2E
*.color7: #FCF3C0
*color7:  #FCF3C0
*.color8: #7B6E63
*color8:  #7B6E63
*.color9: #B7BA25
*color9:  #B7BA25
*.color10: #72C835
*color10:  #72C835
*.color11: #D2859A
*color11:  #D2859A
*.color12: #FA4832
*color12:  #FA4832
*.color13: #82A497
*color13:  #82A497
*.color14: #F9BC2E
*color14:  #F9BC2E
*.color15: #FCF3C0
*color15:  #FCF3C0

! Black color that will not be affected by bold highlighting.
*.color66: #312F2E
*color66:  #312F2E

! Xclock colors.
XClock*foreground: #FCF3C0
XClock*background: #312F2E
XClock*majorColor:  rgba:fc/f3/c0/ff
XClock*minorColor:  rgba:fc/f3/c0/ff
XClock*hourColor:   rgba:fc/f3/c0/ff
XClock*minuteColor: rgba:fc/f3/c0/ff
XClock*secondColor: rgba:fc/f3/c0/ff

! Set depth to make transparency work.
URxvt*depth: 32
