! urxvt configs - so far they are extremely small
URxvt.scrollBar: false
URxvt.font: xft:Office Code Pro Medium:size=11
URxvt.letterSpace: -1

! X colors.
! Generated by 'wal'
*foreground:        #E3E3EE
*background:        #171717
*.foreground:       #E3E3EE
*.background:       #171717
emacs*foreground:   #E3E3EE
emacs*background:   #171717
URxvt*foreground:   #E3E3EE
XTerm*foreground:   #E3E3EE
UXTerm*foreground:  #E3E3EE
URxvt*background:   [100]#171717
XTerm*background:   #171717
UXTerm*background:  #171717
URxvt*cursorColor:  #E3E3EE
XTerm*cursorColor:  #E3E3EE
UXTerm*cursorColor: #E3E3EE
URxvt*borderColor:  [100]#171717

! Colors 0-15.
*.color0: #171717
*color0:  #171717
*.color1: #FFC0CB
*color1:  #FFC0CB
*.color2: #CB8B3B
*color2:  #CB8B3B
*.color3: #F33740
*color3:  #F33740
*.color4: #FEDC32
*color4:  #FEDC32
*.color5: #72C835
*color5:  #72C835
*.color6: #95A5C7
*color6:  #95A5C7
*.color7: #E3E3EE
*color7:  #E3E3EE
*.color8: #51484D
*color8:  #51484D
*.color9: #FFC0CB
*color9:  #FFC0CB
*.color10: #CB8B3B
*color10:  #CB8B3B
*.color11: #F33740
*color11:  #F33740
*.color12: #FEDC32
*color12:  #FEDC32
*.color13: #72C835
*color13:  #72C835
*.color14: #95A5C7
*color14:  #95A5C7
*.color15: #E3E3EE
*color15:  #E3E3EE

! Black color that will not be affected by bold highlighting.
*.color66: #171717
*color66:  #171717

! Xclock colors.
XClock*foreground: #E3E3EE
XClock*background: #171717
XClock*majorColor:  rgba:e3/e3/ee/ff
XClock*minorColor:  rgba:e3/e3/ee/ff
XClock*hourColor:   rgba:e3/e3/ee/ff
XClock*minuteColor: rgba:e3/e3/ee/ff
XClock*secondColor: rgba:e3/e3/ee/ff

! Set depth to make transparency work.
URxvt*depth: 32
